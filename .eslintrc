{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
        "no-eval": 2,
        "no-console": 2,
        "strict": [2, "global"],
        "guard-for-in": 2,
        "wrap-iife": 2,
        "new-cap": 2,
        "no-caller": 2,
        "curly": 2,
        "eqeqeq": 2,
        "no-bitwise": 2,
        "no-empty": 2,
        "no-use-before-define": 2,
        "no-new": 2,
        "max-depth": [2, 3],
        "no-undef": 2,
        "no-unused-vars": 2,
        "max-params": [2, 4],
        "max-statements": [2, 20],
        "complexity": [2, 10],
        "quotes": [2, "single"],
        "no-mixed-requires": 0,
        "consistent-return": 0,
        "no-underscore-dangle": 0,
        "no-shadow": 0,
        "semi": [2, "always"],

        "camelcase": 0,
        "no-trailing-spaces": 2,
        "eol-last": 2,

        "space-after-keywords": [2, "always"],
        "space-before-blocks": [2, "always"],

        "arrow-spacing": [2, {"before": true, "after": true}],
        "constructor-super": 2,
        "generator-star-spacing": [2, {"before": false, "after": true}],
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-this-before-super": 2,
        "no-var": 0,
        "object-shorthand": 1,
        "prefer-arrow-callback ": 0,
        "prefer-const": 2,
        "prefer-spread": 0,
        "prefer-reflect": 0,
        "prefer-template": 1,
        "require-yield": 0
    }
}
